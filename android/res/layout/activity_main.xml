<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"  android:id="@+id/main_layout"  android:layout_width="match_parent"  android:layout_height="match_parent"><ScrollView   android:layout_width="match_parent"  android:layout_height="match_parent">    <LinearLayout      android:padding="16dp"      android:background="@color/tint_dark"      android:orientation="vertical"      android:layout_width="match_parent" android:layout_height="wrap_content">    <ImageView      android:layout_gravity="center_horizontal"      android:id="@+id/logo"      android:src="@drawable/logo"      android:layout_centerHorizontal="true"      android:layout_width="wrap_content" android:layout_height="wrap_content"/>        <LinearLayout            android:orientation="vertical"            android:background="@drawable/card"            android:id="@+id/card_supported_watches"            android:paddingTop="16dp"            android:paddingLeft="16dp"            android:paddingRight="16dp"            android:layout_marginBottom="8dp"            android:layout_width="match_parent" android:layout_height="wrap_content">            <TextView                android:textColor="@color/secondary"                style="?android:attr/textAppearanceMedium"                android:id="@+id/supported_watches_text"                android:text="@string/supported_watches_text"                android:paddingBottom="8dp"                android:layout_width="match_parent" android:layout_height="wrap_content"/>            <TextView                android:textColor="@color/secondary"                style="?android:attr/textAppearanceMedium"                android:id="@+id/unsupported_watches_text"                android:text="@string/unsupported_watches_text"                android:layout_width="match_parent" android:layout_height="wrap_content"/>            <Button                android:textStyle="bold"                android:layout_gravity="right"                android:textColor="@color/primary"                android:background="@drawable/button_flat"                android:id="@+id/supported_wearables_more_info"                android:text="@string/more_info"                android:layout_width="wrap_content" android:layout_height="wrap_content"/>        </LinearLayout>        <LinearLayout            android:id="@+id/card_install_saa"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginBottom="8dp"            android:background="@drawable/card"            android:orientation="vertical"            android:paddingLeft="16dp"            android:paddingRight="16dp"            android:paddingTop="16dp">            <TextView                android:id="@+id/install_saa_text"                style="?android:attr/textAppearanceMedium"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:text="@string/install_saa_text"                android:textColor="@color/secondary" />            <Button                android:id="@+id/install_saa"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="right"                android:background="@drawable/button_flat"                android:text="@string/install"                android:textColor="@color/primary"                android:textStyle="bold" />        </LinearLayout>        <LinearLayout            android:id="@+id/card_install_gear"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginBottom="8dp"            android:background="@drawable/card"            android:orientation="vertical"            android:paddingLeft="16dp"            android:paddingRight="16dp"            android:paddingTop="16dp">            <TextView                android:id="@+id/install_gearfit2_text_title"                style="?android:attr/textAppearanceMedium"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:text="@string/install_gearfit2_text_title"                android:textColor="@color/secondary"                android:textStyle="bold" />            <TextView                android:id="@+id/install_gearfit2_text_subtitle"                style="?android:attr/textAppearanceMedium"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:text="@string/install_gearfit2_text_subtitle"                android:textColor="@color/secondary" />            <Button                android:id="@+id/install_gearfit2"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="right"                android:background="@drawable/button_flat"                android:text="@string/install"                android:textColor="@color/primary"                android:textStyle="bold" />            <TextView                android:id="@+id/install_gear_text_title"                style="?android:attr/textAppearanceMedium"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:text="@string/install_gear_text_title"                android:textColor="@color/secondary"                android:textStyle="bold" />            <TextView                android:id="@+id/install_gear_text_subtitle"                style="?android:attr/textAppearanceMedium"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:text="@string/install_gear_text_subtitle"                android:textColor="@color/secondary" />            <Button                android:id="@+id/install_gear"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="right"                android:background="@drawable/button_flat"                android:text="@string/install"                android:textColor="@color/primary"                android:textStyle="bold" />        </LinearLayout>        <LinearLayout            android:id="@+id/card_setup"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginBottom="8dp"            android:background="@drawable/card"            android:orientation="vertical"            android:paddingLeft="16dp"            android:paddingRight="16dp"            android:paddingTop="16dp">            <TextView                android:id="@+id/setup_text"                style="?android:attr/textAppearanceMedium"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:text="@string/setup_text"                android:textColor="@color/secondary" />            <Button                android:id="@+id/setup"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="right"                android:background="@drawable/button_flat"                android:text="@string/setup"                android:textColor="@color/primary"                android:textStyle="bold" />        </LinearLayout>    </LinearLayout></ScrollView></FrameLayout>